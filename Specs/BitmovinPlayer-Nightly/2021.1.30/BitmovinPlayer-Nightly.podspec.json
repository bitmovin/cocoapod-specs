{
  "name": "BitmovinPlayer-Nightly",
  "version": "2021.1.30",
  "summary": "Bitmovin Player SDK nightly build for iOS and tvOS",
  "homepage": "https://www.bitmovin.com/",
  "social_media_url": "https://twitter.com/bitmovin",
  "documentation_url": "https://www.bitmovin.com/",
  "authors": {
    "Bitmovin": "support@bitmovin.com"
  },
  "license": {
    "type": "Commercial",
    "file": "BitmovinPlayer/LICENSE.md"
  },
  "source": {
    "http": "https://cdn.bitmovin.com/player/ios_tvos/2021.1.30/BitmovinPlayer-Nightly.zip"
  },
  "platforms": {
    "ios": "10.0",
    "tvos": "10.0"
